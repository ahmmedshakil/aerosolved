FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         none;

    div(phi,U)      Gauss vanLeer;

    div(phi,T)      Gauss vanLeer;

    div(mvLimiter)  Gauss vanLeer01;

    div(phic,Y)     Gauss linear;

    div(phi,p1)     Gauss linear;

    div(mu*dev2(T(grad(U)))) Gauss linear;

    div(phi,V)      Gauss upwind;
}

laplacianSchemes
{
    default         Gauss linear uncorrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         uncorrected;
}

fluxRequired
{
    default         no;
    p1;
}

