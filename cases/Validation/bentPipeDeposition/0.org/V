FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      V;
}

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
     	type            groovyBC;
        value           uniform (0 0 0);
        valueExpression "U";
        variables       "";
    }
    walls
    {
        type            zeroGradientDepositionVelocity;
        value           uniform (0 0 0);
        TOL             1E-10;
        Niter           99;
    }
}
